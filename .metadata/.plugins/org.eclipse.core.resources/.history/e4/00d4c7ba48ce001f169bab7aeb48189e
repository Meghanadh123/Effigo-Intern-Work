package Testing.Exercises;

import java.util.HashSet;

public class StringUtilities {
		HashSet<Character> lower=new HashSet<>();
		HashSet<Character> upper=new HashSet<>();
		
		public StringUtilities()
		{
			lower.add('a');
			lower.add('e');
			lower.add('i');
			lower.add('o');
			lower.add('u');
			upper.add('A');
			upper.add('E');
			upper.add('I');
			upper.add('O');
			upper.add('U');
		}
}
